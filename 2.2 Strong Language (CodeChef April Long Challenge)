#include<bits/stdc++.h>
using namespace std;
int main()
{
 int t;
    cin>>t;
    while(t--)
    {
        int n,k;
        cin>>n>>k;
        string s;
        cin>>s;
        int count=0,flag=0;
        for(int i=0;i<s.length();i++)
        {
            if(s[i]=='*')
            {
                count++;
                
            }
            else
            {
                
                count=0;
            }
                if(count>=k)
                {
                    flag=1;
                    break;
                }
                
            }
        if(flag)
        {
            
            cout<<"YES"<<endl;
            
        }
        else
        {
            cout<<"NO"<<endl;
        }
    }
}
