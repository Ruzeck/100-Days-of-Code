string encode(string &message)
{
    
    
  int n =message.length();
    for(int i=0;i<n;i++)
    {
        int count=1;
        while(i<n-1 && message[i]==message[i+1])
        {
            count++;
            i++;
        }
        cout<<message[i]<<count;
    }
    return message;
    
    
}
