Reverse an array.

#include<bits/stdc++.h>
using namespace std;
int main()
{
    int arr[5];
    for(int i=0;i<5;i++)
    {
        cin>>arr[i];
    }
    int start=0;
    int end=4;
while(start<end)
{
    int temp=arr[start];
    arr[start]=arr[end];
    arr[end]=temp;
    start++;
    end--;
}
for(int i=0;i<5;i++)
    {
        cout<<arr[i]<<endl;
    }
    
    return 0;

}
