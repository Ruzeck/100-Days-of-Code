#include<iostream>
#include<string>
using namespace std;
int main()
{
    int t;
    cin>>t;
    string s;
    int count=0;
    while(t--)
    {
       
       cin>>s;
       for(int i=0;i<s.length();i++)
       {
           if(s[i]=='1'&&s[i+1]=='0'||s[i]='1'&& s[i+1]== '\0')
           {
               count++;
           }
           
       }
       cout<<count<<endl;
       }
    }
