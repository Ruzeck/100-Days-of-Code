#include<iostream>
using namespace std;
int
main ()
{
  long long int n, h, x;
  int flag = 0;

  cin >> n >> h >> x;
  int t[n];
  for (int i = 0; i < n; i++)
    {
      cin >> t[i];
    }
  for (int i = 0; i < n; i++)
    {
      if (t[i] + x >= h)
	{
	  flag = 1;
	  break;
	}
    }
  if (flag)
    {
      cout << "YES" << endl;
    }
  else
    {
      cout << "N0" << endl;
    }
